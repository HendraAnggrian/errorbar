<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Widget.Design.Errorbar" parent="android:Widget" tools:ignore="PrivateResource">
        <item name="animationMode">fade</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Medium</item>

        <!-- inherited from Snackbar -->
        <item name="elevation">@dimen/design_snackbar_elevation</item>
    </style>

    <style name="Widget.Design.Errorbar.Cloud" parent="Widget.Design.Errorbar">
        <item name="android:background">@drawable/errorbar_bg_cloud</item>
        <item name="android:src">@drawable/errorbar_ic_cloud</item>
    </style>
</resources>